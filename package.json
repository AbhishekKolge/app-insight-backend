{
  "name": "app-insight-backend",
  "version": "1.0.0",
  "description": "Analytics of applications for free.",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/app.js",
    "dev": "node --watch src/app.js",
    "seed-category": "node seed/category.js",
    "seed-content-rating": "node seed/contentRating.js",
    "seed-genre": "node seed/genre.js",
    "seed-apps": "node seed/apps.js",
    "seed-review": "node seed/review.js",
    "seed": "npm-run-all -s seed-category seed-content-rating seed-genre seed-apps seed-review"
  },
  "keywords": [
    "android",
    "ios",
    "analytics"
  ],
  "author": "Abhishek Kolge",
  "license": "ISC",
  "dependencies": {
    "@joi/date": "^2.1.0",
